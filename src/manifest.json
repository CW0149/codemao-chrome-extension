{
  "name": "班期工具",
  "description": "打开班期工具",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["activeTab", "tabs", "cookies", "scripting", "notifications"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  },
  "host_permissions": ["https://festival.codemao.cn/*", "https://internal-account.codemao.cn/*"]
}